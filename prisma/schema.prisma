generator client {

  provider = "prisma-client-js"

}

datasource db {

  provider = "sqlite"
  url      = env("DATABASE_URL")

}

model Pessoa {

  id  Int @id @default(autoincrement())
  nome  String
  funcao  String
  areaID Int
  area Area @relation(fields: [areaID], references: [id])

}

model Area {

  id  Int @id @default(autoincrement())
  nome  String
  tipo  String
  localizacao String
  pessoas Pessoa[]

}